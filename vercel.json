{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { 
                "distDir": "build"        
            }
      }
    ],
    "routes": [
      { 
        "src": "/remoteEntry.js", 
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "http://localhost:3000"
          },
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ],
        "dest": "build/remoteEntry.js" 
      },
      { "src": "/(.*)", "dest": "build/$1" }
    ]
}

