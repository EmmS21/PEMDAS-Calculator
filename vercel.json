{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "build"
      }
    },
    {
      "src": "./build/remoteEntry.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { 
      "src": "/remoteEntry.js", 
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/javascript"
      },
      "dest": "/build/remoteEntry.js",
      "status": 200 
    },
    { 
      "src": "/(.*)", 
      "dest": "/build/$1" 
    }
  ]
}
