{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
          "distDir": "build"        
      }
    }
  ],
  "routes": [
    { 
      "src": "/remoteEntry.js", 
      "headers": {
          "Access-Control-Allow-Origin": "http://localhost:3000",
          "Content-Type": "application/javascript"
      },
      "dest": "build/remoteEntry.js" 
    },
    { "src": "/(.*)", "dest": "build/$1" }
  ]
}
