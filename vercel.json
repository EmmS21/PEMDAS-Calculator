{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { 
                "distDir": "build", 
                "installCommand": "npm run vercel-install" 
        }
      }
    ],
    "routes": [
      { 
        "src": "/remoteEntry.js", 
        "headers": {
          "Access-Control-Allow-Origin": "*"
        },
        "dest": "build/remoteEntry.js" 
      },
      { "src": "/(.*)", "dest": "build/$1" }
    ]
}

